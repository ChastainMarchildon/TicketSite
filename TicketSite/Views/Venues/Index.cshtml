@model IEnumerable<TicketSite.Models.Venue>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Add a Show", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Venue_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Artist_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tickets)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Venue_Location)
        </th>
        <th></th>
    </tr>

@foreach (var show in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => show.Venue_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => show.Artist_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => show.Tickets)
        </td>
        <td>
            @Html.DisplayFor(modelItem => show.Venue_Location)
        </td>
        <td>
            
            @Html.ActionLink("Details", "Details", new { id=show.VenueID }) 
            
        </td>
    </tr>
}

</table>
