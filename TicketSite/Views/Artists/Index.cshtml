@model IEnumerable<TicketSite.Models.Artist>

@{
    ViewBag.Title = "Artists";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Artist
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Artist_Name)
           
        </td>
        <td>
            @Html.ActionLink("Artists", "Shows", new { id=item.ArtistID }) 
        </td>
    </tr>
}

</table>
